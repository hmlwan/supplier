.input-wrap {
  border: 1rpx solid #eee;
  background-color: #f8f8f8;
  border-radius: 10rpx;
  padding-left: 20rpx;
  margin-top: 10rpx;
  width: 100%;
  height: 65rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  box-sizing: border-box;
}

input {
  margin-left: 40rpx;
  margin-right: 10rpx;
  height: 80rpx;
  width: 78%;
  /* font-size: 26rpx; */
}
.search-wrap {
  display: flex;
  flex-direction: row;
  justify-content: center;
  margin: 20rpx 0;

}
.search-btn {
  color: #eee;
  /* font-size: 26rpx; */
  line-height:70rpx;
  height: 70rpx;
  width: 80%;
  background-color: #0052d9;
  border-radius: 35rpx;
  text-align: center;
}
.result-wrap {
  background-color: #fff;
  border-radius: 10rpx;
  padding: 20rpx 0;
  margin-bottom: 30rpx;
}
.result-tit {
  border-bottom: 1px solid #eee;
  font-weight: bold;
  height: 80rpx;
  line-height: 80rpx;
  padding:0 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.result-body {
  display: flex;
  flex-direction: row;
  min-height: 90rpx;
  border-bottom: 1px solid #eee;
  padding: 20rpx;
  align-items: center;
}
.result-body:last-child {
  border: none;
}
.result-name {
  width: 200rpx;
}
.reuslt-cont {
  flex: 1;
}